{
  "name": "@elaraai/e3-core",
  "version": "0.0.2-beta.8",
  "type": "module",
  "description": "East Execution Engine Core - Programmatic API for e3 repository operations",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    },
    "./test": {
      "types": "./dist/src/test-helpers.d.ts",
      "default": "./dist/src/test-helpers.js"
    }
  },
  "files": [
    "dist/src",
    "!dist/src/**/*.spec.*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc --build",
    "test": "npm run build && node --enable-source-maps --test 'dist/src/**/*.spec.js'",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "east",
    "e3",
    "core",
    "library"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/elaraai/e3",
    "directory": "packages/e3-core"
  },
  "dependencies": {
    "@elaraai/e3": "^0.0.2-beta.8",
    "@elaraai/e3-types": "^0.0.2-beta.8",
    "yauzl": "^3.2.0",
    "yazl": "^2.5.1"
  },
  "peerDependencies": {
    "@elaraai/east": "^0.0.1-beta.22"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/yauzl": "^2.10.3",
    "@types/yazl": "^2.4.5",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "eslint": "^9.0.0",
    "eslint-plugin-headers": "^1.3.3",
    "typescript": "^5.6.0"
  }
}
