  "keywords": [
    "east",
    "e3",
    "execution-engine",
    "monorepo"
  ],
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "eslint": "^9.0.0",
    "eslint-plugin-headers": "^1.3.3",
    "typescript": "^5.6.0"
  }
}
