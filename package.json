{
  "name": "@elaraai/e3",
  "version": "0.0.1-alpha.0",
  "private": true,
  "description": "East Execution Engine - Monorepo for e3 packages",
  "type": "module",
  "workspaces": [
    "packages/*",
    "integration-tests",
    "test"
  ],
  "scripts": {
    "build": "npm run build --workspaces --if-present",
    "test": "npm run test --workspaces --if-present",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "clean": "npm run clean --workspaces --if-present && rm -rf node_modules",
    "fuzz": "npm run fuzz --workspace=e3-fuzz",
    "fuzz:quick": "npm run fuzz:quick --workspace=e3-fuzz",
    "fuzz:stress": "npm run fuzz:stress --workspace=e3-fuzz"
  },
  "keywords": [
    "east",
    "e3",
    "execution-engine",
    "monorepo"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "eslint": "^9.0.0",
    "eslint-plugin-headers": "^1.3.3",
    "typescript": "^5.6.0"
  }
}
