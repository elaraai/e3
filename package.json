{
  "name": "@elaraai/e3",
  "version": "0.0.1-alpha.0",
  "private": true,
  "description": "East Execution Engine - Monorepo for E3 packages",
  "type": "module",
  "workspaces": [
    "e3cli",
    "e3dk",
    "e3-runner-node",
    "e3-types"
  ],
  "scripts": {
    "build": "npm run build --workspaces --if-present",
